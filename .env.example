# Firebase configuration variables
# Copy this file to .env and fill in your actual Firebase project values.
# Get these values from your Firebase project settings > Web app config.
# In Netlify, set these environment variables in the site's build & deploy settings.

VITE_FIREBASE_API_KEY="YOUR_API_KEY"
VITE_FIREBASE_AUTH_DOMAIN="YOUR_AUTH_DOMAIN"
VITE_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
VITE_FIREBASE_STORAGE_BUCKET="YOUR_STORAGE_BUCKET"
VITE_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
VITE_FIREBASE_APP_ID="YOUR_APP_ID"

# You can keep the App ID the same as the one used in Canvas or define a new one
VITE_APP_ID="default-issue-tracker"
